                Margin="130,170,0,0" 
                VerticalAlignment="Top"
                Height="76"
                Width="248"
                FontSize="24" Click="Button_Click"/>
               
        <Button Content="Go To Page 1" HorizontalAlignment="Left" Margin="411,170,0,0" VerticalAlignment="Top" Height="73" Width="284" FontSize="24" Click="Button_Click_1"/>
        <TextBlock x:Name="myTextBlock2" HorizontalAlignment="Left" Margin="115,342,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top" Height="333" Width="675" FontSize="24"/>
        <Button Content="Go Back" HorizontalAlignment="Left" Margin="838,173,0,0" VerticalAlignment="Top" Height="70" Width="250" FontSize="24" Click="Button_Click_2"/>

